{
  "name": "@hg-ui/h-frame",
  "version": "0.1.0",
  "description": "页面框架",
  "scripts": {
    "bootstrap": "pnpm install",
    "start": "npm run bootstrap && npm run dev",
    "dev": "pnpm -C preview dev",
    "build": "npm run build:lib && npm run build:docs",
    "build:lib": "vite build",
    "build:docs": "pnpm -C preview build",
    "lintcss": "stylelint \"src/**/*.scss\" --fix",
    "lint": "eslint --ext .js,.vue src",
    "lintfix": "eslint --fix --ext .js,.vue src",
    "log": "changelog-sn -i CHANGELOG.md -s -r 2",
    "cz": "git add . && git cz",
    "pub": "cd dist && npm publish"
  },
  "keywords": [
    "vue",
    "component",
    "frame",
    "page"
  ],
  "author": "WenHao.Huang(huangwenhao23@163.com)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/WenHaoHuang/demo.git"
  },
  "homepage": "",
  "bugs": {
    "url": "https://github.com/WenHaoHuang/demo/issues"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./.czconfigrc.js"
    }
  },
  "engines": {
    "node": ">= 8.9.1",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "peerDependencies": {
    "vue": "3.2.26",
    "element-plus": "1.3.0-beta.7",
    "@element-plus/icons-vue": "0.2.6",
    "vue-router": "4.0.12"
  },
  "dependencies": {
    "vue": "3.2.26",
    "element-plus": "1.3.0-beta.7",
    "@element-plus/icons-vue": "0.2.6",
    "vue-router": "4.0.12",
    "@vueuse/core": "7.5.4",
    "lodash": "4.17.21",
    "@babel/core": "7.16.12",
    "@types/node": "17.0.10",
    "@vitejs/plugin-vue": "2.1.0",
    "@vitejs/plugin-vue-jsx": "1.3.3",
    "@vue/babel-plugin-jsx": "1.1.1",
    "@vue/compiler-sfc": "3.2.29",
    "@vue/component-compiler-utils": "3.3.0",
    "@vue/eslint-config-typescript": "10.0.0",
    "@vue/runtime-core": "3.2.29",
    "@vue/shared": "3.2.29",
    "autoprefixer": "10.4.2",
    "postcss": "8.4.5",
    "postcss-import": "14.0.2",
    "postcss-url": "10.1.3",
    "sass": "1.49.0",
    "typescript": "4.5.5",
    "vite": "2.7.13",
    "vite-plugin-dts": "0.9.9",
    "vue-tsc": "0.31.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.10.0",
    "@typescript-eslint/parser": "5.10.0",
    "eslint": "8.7.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-vue": "8.3.0",
    "stylelint": "14.3.0",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-recess-order": "3.0.0",
    "stylelint-config-standard": "24.0.0",
    "stylelint-order": "5.0.0",
    "stylelint-scss": "4.1.0"
  }
}
